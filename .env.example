# .env.example
# API Configuration
API_PREFIX=/api/v1
ALLOWED_ORIGINS=["http://localhost:3000","http://localhost:7860","http://localhost:8000"]
HOST=0.0.0.0
PORT=8000

# Device & Performance
DEVICE=auto
MAX_WORKERS=4
MAX_BATCH_SIZE=10
USE_FP16=true
USE_ATTENTION_SLICING=true
CPU_OFFLOAD=false

# Model Settings
MODEL_CACHE_DIR=./models
DEFAULT_CAPTION_MODEL=blip2
DEFAULT_VQA_MODEL=llava
DEFAULT_LLM_MODEL=qwen

# Security & Safety
ENABLE_CONTENT_FILTER=true
ENABLE_NSFW_FILTER=true
ENABLE_FACE_BLUR=false
CONTENT_FILTER_LEVEL=medium
MAX_IMAGE_SIZE_MB=10
MAX_PROMPT_LENGTH=500

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Database (for future phases)
DATABASE_URL=sqlite:///./data/visionquest.db
REDIS_URL=redis://localhost:6379/0